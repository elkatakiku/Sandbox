<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<DialogPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bankManagementSystem.WithdrawDialogController">
    <content>
        <VBox alignment="CENTER" prefHeight="250.0" prefWidth="100.0" spacing="10.0">
            <children>
                <Label fx:id="actionHeaderLabel" alignment="CENTER" prefHeight="74.0" prefWidth="380.0" text="WITHDRAW">
                    <font>
                        <Font name="System Bold" size="20.0" />
                    </font></Label>
                <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="200.0">
                    <children>
                        <Label prefWidth="200.0" text="Account:" />
                        <Label fx:id="accountNameLabel" prefWidth="200.0" text="Account Name">
                     <font>
                        <Font size="13.0" />
                     </font></Label>
                    </children>
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding></HBox>
                <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="37.0" prefHeight="30.0" prefWidth="200.0">
                    <children>
                        <Label prefWidth="200.0" text="Current Balance:" />
                        <Label fx:id="balanceLabel" prefHeight="30.0" prefWidth="200.0" text="Balance" />
                    </children>
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
                </HBox>
                <VBox alignment="CENTER" spacing="10.0">
                    <children>
                        <Label fx:id="actionTypeLabel" text="Withdraw Amount" />
                        <TextField fx:id="amountTextField" alignment="CENTER" maxWidth="300.0" minHeight="35.0" prefHeight="50.0" prefWidth="100.0" promptText="Enter deposit amount" />
                    </children>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
                </VBox>
            </children></VBox>
    </content>
</DialogPane>
